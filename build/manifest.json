{
    "name": "Dark Mode Everything",
    "description": "Dark mode for every website!",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [ 
        {
            "css": [ "content/content.css" ],
            "js": [ "content/darkmode.js" ],
            "matches": [ "<all_urls>" ]
        } 
    ],
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "web_accessible_resources": [
        "content/content.css"
    ],
    "permissions": [
        "tabs"
    ]   
}